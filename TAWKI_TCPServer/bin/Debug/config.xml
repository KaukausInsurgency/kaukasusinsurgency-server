<Config>
	<DBConnect>Server=127.0.0.1; Database=ki; Uid=KITestAcc; Pwd=test;</DBConnect>
	<RedisDBConnect>localhost:16379</RedisDBConnect>
	<RedisEnvironmentKey>KI</RedisEnvironmentKey>
	<RedisActionKeys>
		<Pair Action="AddOrUpdateCapturePoint" RedisKey="CP" />
		<Pair Action="AddOrUpdateDepot" RedisKey="Depot" />
		<Pair Action="AddOrUpdateSideMission" RedisKey="SM" />
		<Pair Action="AddOrUpdateChat" RedisKey="Chat" />
	</RedisActionKeys>
	<Throttle>
		<Action Name="AddOrUpdateCapturePoint" MinSeconds="5" />
		<Action Name="AddOrUpdateDepot" MinSeconds="5" />
		<Action Name="AddOrUpdateSideMission" MinSeconds="5" />
		<Action Name="AddOrUpdateChat" MinSeconds="5" />
		<Action Name="SendHeartbeat" MinSeconds="2" />
	</Throttle>
	<SupportedHTML>p,h1,h2,h3,h4,h5,h6,br,b,strong,i,em,mark,small,del,ins,sub,sup</SupportedHTML>
	<Port>9983</Port>
	<MaxConnections>4</MaxConnections>
	<WhiteList>192.168.1.254;::ffff:127.0.0.1</WhiteList>
	<Version>0.90</Version>
	<VersionKey>AAAA</VersionKey>
</Config>